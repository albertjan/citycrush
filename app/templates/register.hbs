<h1>register</h1>

<div class="row">
        <div class="large-6 small-6 columns">
            <h2>registreren</h2>
        </div>
    </div>
    <div class="row">
        <div class="large-6 small-6 columns">
            <label>naam: </label>
            {{input valueBinding='name'}}
        </div>
    </div>
    <div class="row">
        <div class="large-6 small-6 columns">
            <label>email: </label>
            {{input valueBinding='email'}}
        </div>
    </div>
    <div class="row">
        <div class="large-6 small-6 columns">
            <a class="button medium" {{action 'register'}}>register</a>
        </div>
    </div>
